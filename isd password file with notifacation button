function changeMainPassword(){var newUsername=document.getElementById('newUsername').value;var newPassword=document.getElementById('newPassword').value;var infoMsg=document.getElementById('infoMsg');if(newUsername&&newPassword){window._auth.s(newUsername,newPassword);infoMsg.textContent='‚úì Main password updated successfully! New username: '+newUsername;infoMsg.style.color='#00ff00';infoMsg.style.borderColor='#00ff00';infoMsg.style.backgroundColor='rgba(0, 255, 0, 0.1)';infoMsg.style.display='block';document.getElementById('newUsername').value='';document.getElementById('newPassword').value='';}else{infoMsg.textContent='‚ö† Please enter both username and password!';infoMsg.style.color='#ff0000';infoMsg.style.borderColor='#dc3545';infoMsg.style.backgroundColor='rgba(220, 53, 69, 0.1)';infoMsg.style.display='block';}}

function showAuthenticationSequence(isAdmin){var infoMsg=document.getElementById('infoMsg');var successMsg=document.getElementById('successMsg');var adminPanel=document.getElementById('adminPanel');document.querySelector('button[type="submit"]').disabled=true;var messages=[{text:'üîÑ Authenticating...',delay:0},{text:'üîç Verifying credentials...',delay:1000},{text:'üîê Checking security clearance...',delay:2000},{text:'‚úì Access granted!',delay:3000}];var currentIndex=0;function displayNextMessage(){if(currentIndex<messages.length){var msg=messages[currentIndex];setTimeout(function(){infoMsg.textContent=msg.text;infoMsg.style.color=currentIndex===messages.length-1?'#00ff00':'#00bfff';infoMsg.style.borderColor=currentIndex===messages.length-1?'#00ff00':'#00bfff';infoMsg.style.backgroundColor=currentIndex===messages.length-1?'rgba(0, 255, 0, 0.1)':'rgba(0, 191, 255, 0.1)';infoMsg.style.display='block';currentIndex++;if(currentIndex<messages.length){displayNextMessage();}else{setTimeout(function(){infoMsg.style.display='none';document.getElementById('accessLink').href=window._auth.l();successMsg.style.display='block';var accessTimer=document.getElementById('accessTimer');var timerWarning=document.querySelector('.timer-warning');if(isAdmin){adminPanel.style.display='block';accessTimer.style.display='none';timerWarning.style.display='none';}else{accessTimer.style.display='block';timerWarning.style.display='block';startAccessTimer();}loginAttempts=0;},1000);}},msg.delay);}}displayNextMessage();}

function startAccessTimer(){var accessTimer=document.getElementById('accessTimer');var successMsg=document.getElementById('successMsg');var accessLink=document.getElementById('accessLink');var timeRemaining=10;accessTimer.textContent=timeRemaining;var timerInterval=setInterval(function(){timeRemaining--;accessTimer.textContent=timeRemaining;if(timeRemaining<=0){clearInterval(timerInterval);successMsg.style.display='none';accessLink.href='#';document.getElementById('username').value='';document.getElementById('password').value='';var infoMsg=document.getElementById('infoMsg');infoMsg.textContent='‚ö† Session expired. Please login again.';infoMsg.style.color='#ffaa00';infoMsg.style.borderColor='#ffaa00';infoMsg.style.backgroundColor='rgba(255, 170, 0, 0.1)';infoMsg.style.display='block';document.querySelector('button[type="submit"]').disabled=false;setTimeout(function(){infoMsg.style.display='none';},3000);}},1000);}

function showFailureSequence(){var infoMsg=document.getElementById('infoMsg');var errorMsg=document.getElementById('errorMsg');loginAttempts++;var attemptsRemaining=maxAttempts-loginAttempts;document.querySelector('button[type="submit"]').disabled=true;if(loginAttempts>=maxAttempts){showLockoutScreen();return;}var messages=[{text:'üîÑ Authenticating...',delay:0},{text:'üîç Verifying credentials...',delay:1000},{text:'‚ùå Authentication failed!',delay:2000}];var currentIndex=0;function displayNextMessage(){if(currentIndex<messages.length){var msg=messages[currentIndex];setTimeout(function(){infoMsg.textContent=msg.text;infoMsg.style.color=currentIndex===messages.length-1?'#ff0000':'#00bfff';infoMsg.style.borderColor=currentIndex===messages.length-1?'#dc3545':'#00bfff';infoMsg.style.backgroundColor=currentIndex===messages.length-1?'rgba(220, 53, 69, 0.1)':'rgba(0, 191, 255, 0.1)';infoMsg.style.display='block';currentIndex++;if(currentIndex<messages.length){displayNextMessage();}else{setTimeout(function(){infoMsg.style.display='none';errorMsg.textContent='‚ö† Incorrect username or password! Attempts remaining: '+attemptsRemaining;errorMsg.style.display='block';document.querySelector('button[type="submit"]').disabled=false;},800);}},msg.delay);}}displayNextMessage();}

function showLockoutScreen(){var lockoutScreen=document.getElementById('lockoutScreen');var memeticHazard=document.getElementById('memeticHazard');var countdownTimer=document.getElementById('countdownTimer');document.getElementById('id01').style.display='none';lockoutScreen.style.display='flex';isLocked=true;var countdown=10;countdownTimer.textContent=countdown;var countdownInterval=setInterval(function(){countdown--;countdownTimer.textContent=countdown;if(countdown<=0){clearInterval(countdownInterval);lockoutScreen.style.display='none';memeticHazard.style.display='flex';var memeticImage=document.getElementById('memeticImage');var memeticFallback=document.getElementById('memeticFallback');setTimeout(function(){if(!memeticImage.complete||memeticImage.naturalHeight===0){memeticImage.style.display='none';memeticFallback.style.display='block';}},2000);}},1000);}

function validateLogin(event){event.preventDefault();if(isLocked){return false;}var username=document.getElementById('username').value;var password=document.getElementById('password').value;var inputAdminUsername=document.getElementById('adminUsername').value;var inputAdminPassword=document.getElementById('adminPassword').value;var errorMsg=document.getElementById('errorMsg');var successMsg=document.getElementById('successMsg');var infoMsg=document.getElementById('infoMsg');var adminPanel=document.getElementById('adminPanel');errorMsg.style.display='none';successMsg.style.display='none';infoMsg.style.display='none';adminPanel.style.display='none';var adminFieldsVisible=document.getElementById('adminFields').style.display==='block';if(adminFieldsVisible){if(window._auth.a(inputAdminUsername,inputAdminPassword)){document.getElementById('accessLink').href=window._auth.l();successMsg.style.display='block';adminPanel.style.display='block';var accessTimer=document.getElementById('accessTimer');var timerWarning=document.querySelector('.timer-warning');accessTimer.style.display='none';timerWarning.style.display='none';loginAttempts=0;return false;}else{showFailureSequence();return false;}}else{if(window._auth.c(username,password)){showAuthenticationSequence(false);return false;}else{showFailureSequence();return false;}}}

var notificationMessages=[{icon:'‚ö†Ô∏è',title:'Containment Alert',body:'SCP-173 cell inspection scheduled for 1400 hours.'},{icon:'üîí',title:'Security Update',body:'Site-19 perimeter breach detected. MTF on route.'},{icon:'üì°',title:'Communication',body:'O5 Council meeting in progress. Level 4+ personnel standby.'},{icon:'‚ö†Ô∏è',title:'Class-D Assignment',body:'Monthly termination schedule updated. Review required.'},{icon:'üî¨',title:'Research Notice',body:'SCP-682 termination attempt #47 postponed indefinitely.'},{icon:'üìã',title:'Personnel Update',body:'Dr. Bright banned from coffee machine. Again.'},{icon:'‚ö†Ô∏è',title:'Memetic Hazard',body:'Cognitohazard detected in Sector-7. Amnestics distributed.'},{icon:'üö®',title:'Breach Protocol',body:'SCP-096 containment compromised. Avoid all visual contact.'},{icon:'üì°',title:'Site Status',body:'Foundation satellite network operating at 98% efficiency.'},{icon:'‚ö†Ô∏è',title:'Anomaly Detected',body:'Temporal distortion in Wing-C. Avoid until 2200 hours.'},{icon:'üîí',title:'ISD Alert',body:'Ethics Committee review scheduled for suspected misconduct.'},{icon:'üìã',title:'Staff Notice',body:'Site cafeteria serving Tuesday meatloaf. Proceed with caution.'},{icon:'‚ö†Ô∏è',title:'Keter Alert',body:'SCP-106 last seen 3 hours ago. Report sightings immediately.'},{icon:'üî¨',title:'Lab Report',body:'SCP-999 scheduled for morale boost session in Break Room B.'},{icon:'üì°',title:'Communication',body:'GOC delegation arriving 0900. Maintain standard cover protocols.'},{icon:'üéÆ',title:'‚ñà‚ñà‚ñà-‚ñà‚ñà‚ñà‚ñà GLITCH DETECTED',body:'Ultra Gaming servers experiencing anomalous activity. Gaming sessions 47% more epic than baseline. ‚ñà‚ñà‚ñà SYSTEM STABILITY COMPROMISED ‚ñà‚ñà‚ñà'},{icon:'üéØ',title:'ERROR-ULTRA-GAMING',body:'Ultra Gaming clan detected unauthorized skill levels. Investigating reality-bending K/D ratios. ‚ñà‚ñà‚ñà DATA CORRUPTION DETECTED ‚ñà‚ñà‚ñà STAND BY ‚ñà‚ñà'},{icon:'‚ö°',title:'[CRITICAL SYS