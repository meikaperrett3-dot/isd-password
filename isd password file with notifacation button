<script>
function testNotification() {
  var select = document.getElementById('testNotificationSelect');
  var value = select.value;
  
  if (value === 'easter1') {
    createNotification(chaosInsurgencyQuote, true, false);
  } else if (value === 'easter2') {
    createNotification(administratorQuote, true, false);
  } else {
    var index = parseInt(value);
    var msg = notificationMessages[index];
    var isGlitched = msg.icon === 'üéÆ' || msg.icon === 'üéØ' || msg.icon === '‚ö°';
    createNotification(msg, false, isGlitched);
  }
}
</script>
