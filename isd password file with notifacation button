function flashScreen(color,times){
  var flash=document.createElement('div');
  flash.style.cssText='position: fixed; top: 0; left: 0; width: 100%; height: 100%; background: '+color+'; z-index: 9999; pointer-events: none; opacity: 0;';
  document.body.appendChild(flash);
  
  var count=0;
  var flashInterval=setInterval(function(){
    flash.style.opacity=flash.style.opacity=='0'?'0.3':'0';
    count++;
    if(count>=times*2){
      clearInterval(flashInterval);
      flash.remove();
    }
  },100);
}
</script>

