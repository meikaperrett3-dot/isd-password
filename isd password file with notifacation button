var administratorQuote={icon:'üëÅÔ∏è',title:'O5 TRANSMISSION',body:'"We die in the dark so you can live in the light. Remember that, and remember why we do what we do." - The Administrator'};

function createNotification(data,isEasterEgg,isGlitched){var panel=document.getElementById('notificationPanel');var notification=document.createElement('div');if(isGlitched){notification.className='notification glitched-notification';}else{notification.className=isEasterEgg?'notification easter-egg-notification':'notification';}var now=new Date();var timeStr=String(now.getHours()).padStart(2,'0')+':'+String(now.getMinutes()).padStart(2,'0');notification.innerHTML='<div class="notification-header">'+'<span class="notification-icon">'+data.icon+'</span>'+'<span class="notification-title">'+data.title+'</span>'+'</div>'+'<div class="notification-body">'+data.body+'</div>'+'<div class="notification-time">'+timeStr+'</div>';if(isGlitched){notification.style.animation='glitchNotification 0.2s infinite, slideIn 0.5s ease-out forwards';}else{notification.style.animation='slideIn 0.5s ease-out forwards';}panel.appendChild(notification);var displayTime=isEasterEgg?10000:7000;if(isGlitched){displayTime=9000;}setTimeout(function(){if(isGlitched){notification.style.animation='glitchNotification 0.2s infinite, slideOut 0.5s ease-in forwards';}else{notification.style.animation='slideOut 0.5s ease-in forwards';}setTimeout(function(){notification.remove();},500);},displayTime);}

var lastNotificationTime=0;
var notificationCooldown=10000;

function showRandomNotification(){var currentTime=Date.now();if(currentTime-lastNotificationTime<notificationCooldown){return;}lastNotificationTime=currentTime;var random=Math.random();if(random<0.002){createNotification(administratorQuote,true,false);}else if(random<0.005){createNotification(chaosInsurgencyQuote,true,false);}else{var randomMsg=notificationMessages[Math.floor(Math.random()*notificationMessages.length)];var isGlitched=randomMsg.icon==='üéÆ'||randomMsg.icon==='üéØ'||randomMsg.icon==='‚ö°';createNotification(randomMsg,false,isGlitched);}}

setInterval(showRandomNotification,8000);

setTimeout(showRandomNotification,2000);

function createGlitchBlocks(){var container=document.getElementById('glitchBlocks');for(var i=0;i<8;i++){var block=document.createElement('div');block.className='glitch-block';block.style.left=Math.random()*100+'%';block.style.top=Math.random()*100+'%';block.style.width=(Math.random()*150+50)+'px';block.style.height=(Math.random()*30+5)+'px';block.style.animationDelay=(Math.random()*5)+'s';container.appendChild(block);}}

function createPixelCorruption(){var body=document.body;setInterval(function(){if(Math.random()>0.7){var pixel=document.createElement('div');pixel.className='pixel-corruption';pixel.style.left=Math.random()*100+'%';pixel.style.top=Math.random()*100+'%';body.appendChild(pixel);setTimeout(function(){pixel.remove();},300);}},200);}

document.addEventListener('DOMContentLoaded',function(){createGlitchBlocks();createPixelCorruption();});

var hiddenCommandBuffer='';
var commandTimeout;

document.addEventListener('keypress',function(e){
  clearTimeout(commandTimeout);
  hiddenCommandBuffer+=e.key.toLowerCase();
  
  commandTimeout=setTimeout(function(){
    hiddenCommandBuffer='';
  },2000);
  
  checkHiddenCommands();
});

function checkHiddenCommands(){
  var infoMsg=document.getElementById('infoMsg');
  
  if(hiddenCommandBuffer.includes('096')){
    infoMsg.innerHTML='<div style="color: #ff0000; font-size: 16px; font-weight: bold;">‚ö† SCP-096 BREACH ALERT ‚ö†</div><div style="margin-top: 8px;">DO NOT VIEW FACIAL FEATURES. SCRAMBLE GEAR REQUIRED. MTF DISPATCHED.</div>';
    infoMsg.style.color='#ff0000';
    infoMsg.style.borderColor='#ff0000';
    infoMsg.style.backgroundColor='rgba(255, 0, 0, 0.2)';
    infoMsg.style.display='block';
    flashScreen('#ff0000',3);
    hiddenCommandBuffer='';
    setTimeout(function(){infoMsg.style.display='none';},5000);
  }
  
  else if(hiddenCommandBuffer.includes('scramble')){
